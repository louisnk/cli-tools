Host *.kyc.qa
  IdentityFile ~/.ssh/kyc-bastion-dev.pem
  User ec2-user
  UseKeychain yes
  AddKeysToAgent yes
  Port 22

Host *.kyc.dev
  IdentityFile ~/.ssh/kyc-bastion-dev.pem
  UseKeychain yes
  AddKeysToAgent yes
  Port 22
  User ec2-user

